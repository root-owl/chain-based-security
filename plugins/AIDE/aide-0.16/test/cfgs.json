{
    "toolName":"AIDE",
    "version":"0.16",
    "whiteListKeys":
    [
        "permissions",
        "fileType",
        "inode",
        "numberOfLinks",
        "linkName",
        "user",
        "group",
        "size",
        "blockCount",
        "mTime",
        "aTime",
        "cTime",
        "growingSize",
        "fileName",
        "content",
        "everything"
    ],
    "supportAlgos":
    [
        "md5",
        "sha1",
        "sha256",
        "sha512",
        "rmd160",
        "tiger",
        "haval",
        "crc32"
    ],
    "contentHashAlgo":"sha512",
    "whiteListRules":
    [
        {
            "whiteListRuleName":"sysDir",
            "whiteListkeys":["everything"]
        },
        {
            "whiteListRuleName":"sysFile",
            "whiteListkeys":["everything"]
        },
        {
            "whiteListRuleName":"logs",
            "whiteListkeys":["inode", "numberOfLinks", "linkName", "size", "blockCount", "mTime", "aTime", "cTime", "growingSize", "content"]
        },
        {
            "whiteListRuleName":"dbFile",
            "whiteListkeys":["inode", "numberOfLinks", "linkName", "size", "blockCount", "mTime", "aTime", "cTime", "growingSize", "content"]
        },
        {
            "whiteListRuleName":"dbDir",
            "whiteListkeys":["inode", "numberOfLinks", "linkName", "size", "blockCount", "mTime", "aTime", "cTime", "growingSize", "content"]
        }
    ],
    "whiteListGroups":
    [
        {
            "groupPrefix":"/home",
            "whiteListEntrys":
            [
                {
                    "path":"/home/ray/.bash_history",
                    "whiteListRuleName":"logs"
                },
                {
                    "path":"/home/ray/.viminfo",
                    "whiteListRuleName":"logs"
                }
            ]
        },
        {
            "groupPrefix":"/usr",
            "whiteListEntrys":
            [
                {
                    "path":"/usr/local/etc/aideDB/aide.db",
                    "whiteListRuleName":"dbFile"
                },
                {
                    "path":"/usr/local/etc/aideDB",
                    "whiteListRuleName":"dbDir"
                }
            ]
        }
    ]
}
